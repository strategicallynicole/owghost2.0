                    <div class="layer" data-depth="0.25"><img style="transform: rotate(14deg);"
                            src="{{asset "images/elements/purpleoak.svg"}}" alt="" width="75" />
                    </div>
                </div>
                <div class="layer-05">
                    <div class="layer" data-depth="0.15"><img style="transform: rotate(-14deg);"
                            src="{{asset "images/elements/redleaf.svg"}}" alt="" width="65" />
                    </div>
                </div>
